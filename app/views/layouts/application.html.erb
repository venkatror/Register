<!DOCTYPE html>
<html>
  <head>
    <title>Profile</title>
    <%= stylesheet_link_tag    "application" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div class="header">
      <div id="com_name">
         </div>
            
    </div>
    <div id="container">
      <div id="content">
      <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, :id => "flash_#{name}" %>
      <% end %>
      <%= yield %>
    </div>
    </div>
    <div class="footer">
    </div>
  </body>
</html>
